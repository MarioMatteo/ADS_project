<?xml version="1.0" encoding="utf-8"?>
<automaton>
	<states>
		<state initial="true">A</state>
		<state>C</state>
		<state>B</state>
	</states>
	<transitions>
		<transition observable="true">
			<source>A</source>
			<event>a</event>
			<destination>C</destination>
		</transition>
		<transition fault="true" observable="true">
			<source>A</source>
			<event>a</event>
			<destination>B</destination>
		</transition>
		<transition observable="true">
			<source>C</source>
			<event>b</event>
			<destination>C</destination>
		</transition>
		<transition observable="true">
			<source>B</source>
			<event>a</event>
			<destination>B</destination>
		</transition>
	</transitions>
</automaton>
