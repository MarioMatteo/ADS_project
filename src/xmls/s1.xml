<?xml version="1.0" encoding="utf-8"?>
<automaton>
	<states>
		<state>B,D</state>
		<state>B,B</state>
		<state initial="true">A,A</state>
	</states>
	<transitions>
		<transition ambiguous="true" observable="true">
			<source>B,D</source>
			<event>a</event>
			<destination>B,D</destination>
		</transition>
		<transition observable="true">
			<source>B,D</source>
			<event>a</event>
			<destination>B,B</destination>
		</transition>
		<transition observable="true">
			<source>B,B</source>
			<event>a</event>
			<destination>B,B</destination>
		</transition>
		<transition ambiguous="true" observable="true">
			<source>B,B</source>
			<event>a</event>
			<destination>B,D</destination>
		</transition>
		<transition observable="true">
			<source>A,A</source>
			<event>a</event>
			<destination>B,B</destination>
		</transition>
	</transitions>
</automaton>
