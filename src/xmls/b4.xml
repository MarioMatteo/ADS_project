<?xml version="1.0" encoding="utf-8"?>
<automaton>
	<states>
		<state initial="true">A</state>
		<state>B</state>
		<state>D</state>
	</states>
	<transitions>
		<transition observable="true">
			<source>A</source>
			<event>a</event>
			<destination>B</destination>
		</transition>
		<transition observable="true">
			<source>B</source>
			<event>a</event>
			<destination>B</destination>
		</transition>
		<transition fault="true" observable="true">
			<source>B</source>
			<event>a</event>
			<destination>D</destination>
		</transition>
		<transition observable="true">
			<source>D</source>
			<event>a</event>
			<destination>B</destination>
		</transition>
		<transition fault="true" observable="true">
			<source>D</source>
			<event>a</event>
			<destination>D</destination>
		</transition>
	</transitions>
</automaton>
