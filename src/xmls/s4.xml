<?xml version="1.0" encoding="utf-8"?>
<automaton>
	<states>
		<state initial="true">A,A</state>
		<state>B,C</state>
		<state>C,C</state>
	</states>
	<transitions>
		<transition ambiguous="true" observable="true">
			<source>A,A</source>
			<event>a</event>
			<destination>B,C</destination>
		</transition>
		<transition observable="true">
			<source>A,A</source>
			<event>a</event>
			<destination>C,C</destination>
		</transition>
		<transition observable="true">
			<source>C,C</source>
			<event>c</event>
			<destination>C,C</destination>
		</transition>
	</transitions>
</automaton>
